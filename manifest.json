{
    "manifest_version": 2,
    "name": "Coding Train PopUp 2.1",
    "version": "2.0",
    "content_scripts": [{
        "matches":[
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "permissions":["tabs"],
    "browser_action": {
        "default_icon": "ct.png", 
        "default_popup": "sketch/index.html",
        "default_title": "ReWrite Every Paragraph Once"
    }
    
}