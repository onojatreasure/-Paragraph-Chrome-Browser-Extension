{
    "manifest_version": 2,
    "name": "Word Definition Extension",
    "version": "2.0",
    "content_scripts": [{
        "matches":[
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "ct.png", 
        "default_popup": "sketch/index.html",
        "default_title": "Look up a definition!"
    }
    
}